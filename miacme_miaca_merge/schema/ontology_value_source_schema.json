            "format": "uri"
        },
        "ontologySource": {
            "type": "string"
        },
        "@type": {
            "description": "The JSON-LD type",
            "type": "string",
            "format": "uri"
        },
        "comment": {
            "type": "string"
        },
        "ontologyValue": {
            "type": "string"
        }
    },
    "id": "https://w3id.org/mircat/miacme_miaca_merge/schema/ontology_value_source_schema.json",
