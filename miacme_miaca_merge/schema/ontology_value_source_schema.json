{
    "$schema": "http://json-schema.org/draft-04/schema",
    "title": "MIACA ontology value source schema",
    "type": "object",
    "properties": {
        "@context": {
            "anyOf": [
                {
                    "type": "string"
                },
                {
                    "type": "object"
                },
                {
                    "type": "array"
                }
            ],
