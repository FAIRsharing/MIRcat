{
    "description": "JSON-schema representing an investigation in MIACME",
    "$schema": "http://json-schema.org/draft-04/schema",
    "id": "https://w3id.org/mircat/miacme_miaca_merge/schema/data_schema.json",
    "version": "1.0",
    "properties": {
        "@context": {
            "description": "The JSON-LD context",
            "anyOf": [
                {
                    "type": "string"
                },
                {
                    "type": "object"
                },
                {
                    "type": "array"
                }
            ]
        },
        "@type": {
            "description": "The JSON-LD type",
            "enum": [
                "Data"
            ],
            "type": "string"
        },
        "processedImages": {
            "type": "string"
        },
        "rawImages": {
            "type": "string"
        },
        "extractedFeatures": {
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "@id": {
            "description": "The JSON-LD identifier",
            "format": "uri",
            "type": "string"
        }
    },
    "type": "object",
    "title": "MIACME data schema"
}