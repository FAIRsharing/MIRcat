{
    "$schema": "http://json-schema.org/draft-04/schema",
    "title": "MIACA composite reagent schema",
    "type": "object",
    "properties": {
        "@context": {
            "anyOf": [
                {
                    "type": "string"
                },
                {
                    "type": "object"
                },
                {
                    "type": "array"
                }
            ],
            "description": "The JSON-LD context"
        },
        "@id": {
            "description": "The JSON-LD identifier",
            "type": "string",
            "format": "uri"
        },
        "reagentList": {
            "items": {
                "$ref": "reagent_list_schema.json"
            },
            "type": "array"
        },
        "compReagentName": {
            "description": "name of cell culture medium",
            "type": "string"
        },
        "@type": {
            "description": "The JSON-LD type",
            "type": "string",
            "format": "uri"
        },
        "comment": {
            "description": "comment",
            "type": "string"
        }
    },
    "id": "https://w3id.org/mircat/miaca_miflowcyt_merge/schema/composite_reagent_schema.json",
    "description": "Composite Reagent. An element to describe the cell line used during the project."
}