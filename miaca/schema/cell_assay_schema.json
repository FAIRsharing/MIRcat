{
  "id" : "https://w3id.org/mircat/miaca/cell_assay_schema.json",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "MIACA cell assay schema",
  "description": "The actual experimental part describing the performed steps, like  treatments, perturbation(s) and data acquisition in necessary detail.",
  "type": "object",
  "properties": {
    "@context": {
      "description": "The JSON-LD context",
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "object"
        },
        {
          "type": "array"
        }
      ]
    },
    "@id": {
      "description": "The JSON-LD identifier",
      "type": "string",
      "format": "uri"
    },
    "@type": {
      "description": "The JSON-LD type",
      "type": "string",
      "format": "uri"
    },
    "ID": {
      "type": "string"
    },
    "treamtment": {
      "$ref": "treatment_schema.json"
    },
    "postTreatment": {
      "type": "string"
    },
    "dataAcquisition": {
      "type": "string"
    }
  }
}