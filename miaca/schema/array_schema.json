{
  "id": "https://w3id.org/mircat/miaca/schema/array_schema.json",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "MIACA array schema",
  "description": "JSON-schema representing MIACA array type.",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "@context": {
      "description": "The JSON-LD context",
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "object"
        },
        {
          "type": "array"
        }
      ]
    },
    "@id": {
      "description": "The JSON-LD identifier",
      "type": "string",
      "format": "uri"
    },
    "@type": {
      "description": "The JSON-LD type",
      "type": "string",
      "format": "uri"
    },
    "ID": {
      "description": "The identifier of the array",
      "type": "string"
    },
   "arraySupport": {
     "description": "reference to a vesseltype",
     "type": "string"
   },
   "xdim": {
     "description": "column",
     "type": "integer"
   },
   "ydim": {
     "description": "row",
     "type": "integer"
   },
   "array":{
     "description": "description of array type and vessel",
     "$ref": "array_schema.json#"
   }
  }
}