{
  "id": "https://w3id.org/mircat/miaca/schema/compound_schema.json",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "MIACA Compound",
  "description": "",
  "type": "object",
  "properties": {
    "@context": {
      "description": "The JSON-LD context",
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "object"
        },
        {
          "type": "array"
        }
      ]
    },
    "@id": {
      "description": "The JSON-LD identifier",
      "type": "string",
      "format": "uri"
    },
    "@type": {
      "description": "The JSON-LD type",
      "type": "string",
      "format": "uri"
    },
    "purity": {
      "description": "% of compounds with >80% purity",
      "type": "string"
    },
    "lipinski": {
      "description": "% of compound comply with Lipinski rule-of-5",
      "type": "string"
    },
    "weight": {
      "description": "Average molecular weight",
      "type": "string"
    }
  }
}
